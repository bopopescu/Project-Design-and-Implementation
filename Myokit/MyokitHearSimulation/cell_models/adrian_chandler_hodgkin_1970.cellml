<!-- Model description -->
  <documentation xmlns="http://cellml.org/tmp-documentation">
    <article>
      <articleinfo>
        <title>Voltage Clamp Experiments in Striated Muscle Fibres</title>
        <author>
          <firstname>Catherine</firstname>
          <surname>Lloyd</surname>
          <affiliation>
